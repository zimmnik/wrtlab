<network>
  <name>wrtlab-moswrt-test-wan</name>
  <forward mode="nat"/>
  <bridge name="virbr102" stp="off"/>
  <ip address='10.64.0.254' netmask='255.255.255.0'>
    <dhcp>
      <range start='10.64.0.1' end='10.64.0.100'/>
      <host mac="__WRTLAB_WAN_MAC__" name="wrtlab-__NAME__-test.local" ip="10.64.0.253"/> 
    </dhcp>
  </ip>
  <domain name="local" localOnly='yes'/>
  <dns>
    <host ip='10.64.0.254'>
      <hostname>__HOSTNAME__.local</hostname>
    </host>
  </dns>
</network>
